<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="product_label_layout_form" model="ir.ui.view">
        <field name="name">print label wizard inherit</field>
        <field name="model">product.label.layout</field>
        <field name="inherit_id" ref="product.product_label_layout_form"/>
        <field name="arch" type="xml">
            <xpath expr="//form/group/group[2]/field[@name='extra_html']" position="attributes">
                <attribute name="attrs">{'invisible': [('print_format', 'not in', ('dymo', '2x7xprice', 'dymo4_6'))]}</attribute>
            </xpath>
        </field>
    </record>
</odoo>
