<?xml version="1.0" encoding="UTF-8" ?>

<odoo>

    <record id="contact_label_report_paperformat" model="report.paperformat">
        <field name="name">Contact Label Report</field>
        <field name="default" eval="True" />
        <field name="format">custom</field>
        <field name="page_height">57</field>
        <field name="page_width">32</field>
        <field name="orientation">Landscape</field>
        <field name="margin_top">0</field>
        <field name="margin_bottom">0</field>
        <field name="margin_left">0</field>
        <field name="margin_right">0</field>
        <field name="disable_shrinking" eval="True"/>
        <field name="dpi">96</field>
    </record>

    <record id="contact_lable_pdf" model="ir.actions.report">
        <field name="name"> Contact Label (PDF)</field>
        <field name="model">res.partner</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">sd_contact_label.contact_label_report</field>
        <field name="report_file">sd_contact_label.contact_label_report</field>
        <field name="binding_model_id" ref="base.model_res_partner"/>
        <field name="paperformat_id" ref="sd_contact_label.contact_label_report_paperformat"/>
        <field name="print_report_name">'Contact Labels - %s' % (object.name)</field>
        <field name="binding_type">report</field>
    </record>

</odoo>

